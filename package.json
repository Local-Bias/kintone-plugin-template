{
  "name": "kintone-plugin-template",
  "version": "1.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@kintone/dts-gen": "6.1.27",
    "@kintone/plugin-packer": "6.0.33",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.9",
    "@konomi-app/kbuild": "^1.0.0",
    "@konomi-app/kintone-utilities": "^1.0.0",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.15",
    "@sentry/react": "7.46.0",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "concurrently": "^7.6.0",
    "dotenv": "16.0.3",
    "eslint": "8.37.0",
    "immer": "9.0.21",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "react-feather": "^2.0.10",
    "recoil": "0.7.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
