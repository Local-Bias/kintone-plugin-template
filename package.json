{
  "name": "kintone-plugin-template",
  "version": "2.0.1",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "plugin init && plugin genkey",
    "build": "concurrently \"npm:build:*\" && plugin zip",
    "build:js": "kbuild prod plugin --prod-root .plugin/contents",
    "build:css": "tailwindcss -i ./src/styles/global.css -o ./.plugin/contents/config.css -c ./tailwind.config.config.mjs --minify && tailwindcss -i ./src/styles/global.css -o ./.plugin/contents/desktop.css -c ./tailwind.config.desktop.mjs --minify",
    "standalone": "concurrently \"npm:build:*\" && plugin zip --env standalone",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:plugin": "plugin dev",
    "dev:css-config": "tailwindcss -i ./src/styles/global.css -o ./.plugin/dev/config.css -c ./tailwind.config.config.mjs -w",
    "dev:css-desktop": "tailwindcss -i ./src/styles/global.css -o ./.plugin/dev/desktop.css -c ./tailwind.config.desktop.mjs -w",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "dependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@konomi-app/kintone-utilities": "*",
    "@konomi-app/kintone-utilities-react": "*",
    "@konomi-app/ui": "*",
    "@konomi-app/ui-react": "*",
    "@mui/icons-material": "^5",
    "@mui/material": "^5",
    "@radix-ui/react-alert-dialog": "^1",
    "@radix-ui/react-dialog": "^1",
    "@radix-ui/react-slot": "^1",
    "class-variance-authority": "^0",
    "clsx": "^2",
    "esbuild": "^0.21.3",
    "immer": "^10",
    "lucide-react": "^0",
    "luxon": "^3",
    "notistack": "^3",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "^4",
    "recoil": "^0",
    "tailwind-merge": "^2",
    "tailwindcss-animate": "^1"
  },
  "devDependencies": {
    "@kintone/dts-gen": "^8",
    "@kintone/plugin-uploader": "^9.1.1",
    "@konomi-app/kbuild": "*",
    "@konomi-app/kintone-plugin": "*",
    "@types/luxon": "^3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "concurrently": "^8",
    "devcert": "^1",
    "dotenv": "^16",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2",
    "eslint-plugin-n": "^17",
    "eslint-plugin-promise": "^6",
    "eslint-plugin-react": "^7",
    "express": "^4",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}