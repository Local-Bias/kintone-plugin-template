{
  "name": "kintone-plugin-template",
  "version": "1.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin --prod-root cdn",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@kintone/dts-gen": "6.1.28",
    "@kintone/plugin-packer": "6.0.34",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.10",
    "@konomi-app/kbuild": "^1.5.1",
    "@konomi-app/kintone-utilities": "^2.4.0",
    "@konomi-app/kintone-utility-component": "^0.8.2",
    "@konomi-app/ui": "^1.6.1",
    "@konomi-app/ui-react": "^1.1.0",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.1",
    "@sentry/react": "7.49.0",
    "@types/luxon": "3.3.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "concurrently": "^8.0.1",
    "dotenv": "16.0.3",
    "eslint": "8.39.0",
    "immer": "10.0.1",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "react-feather": "^2.0.10",
    "recoil": "0.7.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
